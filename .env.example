# Copy this file to .env and fill in the values
# Required for Gemini text generation
GEMINI_API_KEY=
# or
# GOOGLE_GEMINI_API_KEY=

# Optional: for TTS endpoints
OPENAI_TTS_API_KEY=

# Port (Render provides PORT automatically)
# PORT=3000

# PostgreSQL connection string (e.g. postgres://user:pass@host:5432/dbname)
DB_HOST=

# Stripe configuration
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_ID=
